            </isNotEmpty>
            <isNotEmpty prepend="and" property="shortName">  
                (a.short_name like '%$shortName$%')
            </isNotEmpty>
            <isNotEmpty prepend="and" property="manager">  
                (a.manager like '%$manager$%')
            </isNotEmpty>
            <isNotEmpty prepend="and" property="name">  
                (a.full_name like '%$name$%' or a.short_name like '%$name$%')
            </isNotEmpty>
            <isNotEmpty prepend="and" property="pny">  
                (a.full_name like '%$pny$%' or a.short_name like '%$pny$%' or a.code like '%$pny$%' or a.py_name like '%$pny$%')
