    <!--
    	调拨入库后，更新对方受理单的 has_in_qty 数量
    	直发
    -->
    <update id="updatePjAllotAcceptInQty" parameterClass="java.util.HashMap" >
		update pj_allot_accept_detail a
		inner join pj_allot_accept_main b
		on a.main_id = b.id
		inner join (
		select b.code as code, a.order_detail_id as orderDetailId, 
		       sum(a.sell_qty) as sellQty, sum(a.out_to_in_qty) as outToInQty
		from pj_out_detail_chk a
		inner join pj_out_main_chk b
		on a.main_id = b.id
		GROUP BY b.code, a.order_detail_id) c
		on b.service_id = c.code and a.id = c.orderDetailId
		set a.has_in_qty = c.outToInQty
		where b.code = #applyServiceId#
	</update>
	
	<!--
    	调拨入库后，更新对方受理单的 has_in_qty 数量
    	非直发 更新调拨出库
    -->
    <update id="updatePjAllotAcceptInQtyT" parameterClass="java.util.HashMap" >
		update pj_allot_accept_detail a
		inner join pj_allot_accept_main b
		on a.main_id = b.id
		inner join (
		select b.code as code, a.order_detail_id as orderDetailId, 
		       sum(a.sell_qty) as sellQty, sum(a.out_to_in_qty) as outToInQty
		from pj_out_detail_chk a
		inner join pj_out_main_chk b
		on a.main_id = b.id
		GROUP BY b.code, a.order_detail_id) c
		on b.service_id = c.code and a.id = c.orderDetailId
		inner join pj_allot_accept_main d
		on b.code = d.service_id
		set a.has_in_qty = c.outToInQty
		where d.code = #applyServiceId#
	</update>
	
	<!--
    	调拨入库后，更新对方受理单的 has_in_qty 数量
    	非直发 更新调拨受理
    -->
    <update id="updatePjAllotAcceptInQtyF" parameterClass="java.util.HashMap" >
		update pj_allot_accept_detail a
		inner join pj_allot_accept_main b
		on a.main_id = b.id
		inner join (
				select b.code as code, a.source_detail_id as sourceDetailId, 
							 sum(has_in_qty) as hasInQty
				from pj_allot_accept_detail a
				inner join pj_allot_accept_main b
				on a.main_id = b.id
				inner join (
						select b.code as code, a.order_detail_id as orderDetailId, 
									 sum(a.sell_qty) as sellQty, sum(a.out_to_in_qty) as outToInQty
						from pj_out_detail_chk a
						inner join pj_out_main_chk b
						on a.main_id = b.id
						GROUP BY b.code, a.order_detail_id) c
				on b.service_id = c.code and a.id = c.orderDetailId
		group by b.code, a.source_detail_id) c
		on b.service_id = c.code and a.id = c.sourceDetailId
		set a.has_in_qty = c.hasInQty
		where b.code = #applyServiceId#
	</update>
	
	
