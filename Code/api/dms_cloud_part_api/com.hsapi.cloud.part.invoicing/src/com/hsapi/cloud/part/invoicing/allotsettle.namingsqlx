	<!-- 更新调拨申请的调整数量 
		 如果对方调拨出库后，本机构完全入库，并且调整掉剩余的未受理数量，则更新调拨申请单为全部入库，全部受理
	 -->
    <update id="updateAllotApplyAdjustQty" parameterClass="java.util.HashMap" >
		update pj_allot_apply_detail 
		set is_allot_finished = case when has_cancel_qty + #adjQty# + has_accept_qty >= apply_qty then 1 else 0 end,
		    allot_finished_date = case when has_cancel_qty + #adjQty# + has_accept_qty >= apply_qty then sysdate() else null end,
		    has_cancel_qty = has_cancel_qty + #adjQty#,
		    is_finished = case when has_in_qty + has_cancel_qty = apply_qty then 1 else 0 end,
		    finish_date = case when has_in_qty + has_cancel_qty = apply_qty then SYSDATE() else NULL end
		where main_id = #mainId# and id = #detailId#
	</update>

