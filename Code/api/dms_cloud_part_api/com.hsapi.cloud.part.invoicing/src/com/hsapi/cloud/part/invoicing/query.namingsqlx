                    A.out_date &gt;= #sOutDate#
                </isNotEmpty>
                <isNotEmpty prepend="and" property="eOutDate">  
                    (A.out_date &lt; #eOutDate#)
                </isNotEmpty>
                <!--创建日期 -->
