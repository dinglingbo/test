	                (b.rtnable_qty > 0)
	            </isNotEmpty>
                <isNotEmpty prepend="order by" property="sortField">  
	                $sortField$ $sortOrder$
	            </isNotEmpty> 
            </dynamic> 
				
	</select>
    
