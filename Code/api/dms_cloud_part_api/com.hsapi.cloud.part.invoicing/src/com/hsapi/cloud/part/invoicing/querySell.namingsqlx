			M.payModeId,
			M.taxSign,
			M.taxRate,
			M.taxAmt,
			M.noTaxAmt,
			M.TaxDiff,
			M.enterAmt,
			M.outQty,
			M.outItem,
			M.outAmt,
			M.sendTypeId,
			M.printTimes,
			M.printManId,
			M.printMan,
			M.printDate,
			M.auditSign,
			M.auditorId,
			M.auditor,
			M.auditDate,
			M.remark,
			M.versionNo,
			M.creatorId,
