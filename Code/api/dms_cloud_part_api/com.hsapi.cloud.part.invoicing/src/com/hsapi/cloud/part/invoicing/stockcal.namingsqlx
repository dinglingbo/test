	<!--采购订单提交后更新在途数量  增加-->
	<update id="updatePartStockOnroadQtyByPchsA" parameterClass="java.util.HashMap" >
		update pj_part_stock a
		inner join (
		  select part_id, sum(order_qty) as orderQty
		  from pj_pchs_order_detail_chk a
		  where main_id = #id#
		  group by part_id
		) b
		on a.part_id = b.part_id
		set a.on_road_qty = a.on_road_qty + b.orderQty
	</update>
	
	<update id="updatePartStoreStockOnroadQtyByPchsA" parameterClass="java.util.HashMap" >
		update pj_part_store_stock a
		inner join (
		  select part_id, store_id, sum(order_qty) as orderQty
		  from pj_pchs_order_detail_chk a
		  where main_id = #id#
		  group by part_id, store_id
		) b
		on a.store_id = b.store_id and a.part_id = b.part_id
		set a.on_road_qty = a.on_road_qty + b.orderQty
	</update>
	
