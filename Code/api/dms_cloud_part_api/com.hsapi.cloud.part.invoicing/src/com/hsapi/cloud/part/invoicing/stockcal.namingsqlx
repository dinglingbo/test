			SELECT SUM(B.TAX_AMT), SUM(B.NO_TAX_AMT), SUM(B.ENTER_AMT), SUM(B.SELL_AMT), SUM(B.SELL_QTY), COUNT(B.ID),
		         SUM(B.TAX_AMT) - SUM(B.NO_TAX_AMT)
			from PJ_OUT_DETAIL B
			where A.ID = B.MAIN_ID
			
				<isNotNull prepend = "" property = "mainId">
