			</isNotNull>
			
			<isNotNull prepend = "" property = "partId">
				AND C.PART_ID = #partId#
			</isNotNull>
			
			<isNotNull prepend = "" property = "code">
				AND (g.QUERY_CODE like '%$code$%' or g.code like '%$code$%')
			</isNotNull>
			
			<isNotNull prepend = "" property = "name">
