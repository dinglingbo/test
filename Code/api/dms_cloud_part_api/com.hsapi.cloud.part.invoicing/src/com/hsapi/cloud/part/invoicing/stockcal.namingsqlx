				</isNotNull>
				
			GROUP BY B.MAIN_ID
		)  
		WHERE EXISTS(
			SELECT 1 FROM PJ_OUT_DETAIL B WHERE B.MAIN_ID = A.ID
		
			<isNotNull prepend = "" property = "mainId">
					AND B.MAIN_ID = #mainId#					
			</isNotNull>
		)
	</update>
	
