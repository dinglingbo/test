			a.car_no AS carNo,
			a.car_vin AS carVin,
			a.bill_main_id AS billMainId,
			a.bill_service_id AS billServiceId,
			a.bill_code AS billCode,
			a.bill_dc AS billDc,
			a.bill_type_id as billTypeId,
			a.rp_amt AS rpAmt,
