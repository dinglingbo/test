				</isNotEmpty>
				<isNotEmpty prepend="and" property="guestId">  
	                (a.guest_id = #guestId#)
	            </isNotEmpty>
	                        
	            <isNotEmpty prepend="and" property="operatorId">  
	                (a.operator_id = #operatorId#)
	            </isNotEmpty>
				
				<isNotEmpty prepend="and" property="operator">  
