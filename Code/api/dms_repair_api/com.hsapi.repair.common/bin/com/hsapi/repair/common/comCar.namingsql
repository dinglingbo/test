<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="com.hsapi.repair.common.comCar">
    <parameterMap class="commonj.sdo.DataObject" id="parameterMap">
        <parameter javaType="date" jdbcType="DATE" property="dateType"/>
    </parameterMap>
    <resultMap class="commonj.sdo.DataObject" id="resultMap">
        <result column="TYPEID" javaType="string" property="typeId"/>
    </resultMap>
    <select id="queryCarBrand" parameterClass="java.util.HashMap" resultClass="commonj.sdo.DataObject">
		SELECT
			a.id nodeId,
			a.car_brand_zh nodeName,
			a.first_code firstCode,
			a.image_url imageUrl,
			'carBrand' nodeType,
			false expanded,
			false isLeaf
		FROM
			com_car_brand a
		where 1 = 1
		<isNotNull property="firstCode">AND a.first_code = #firstCode#</isNotNull>
    </select>
    <select id="queryCarSeriesByBrandId" parameterClass="java.util.HashMap" resultClass="commonj.sdo.DataObject">SELECT
			a.id nodeId,
			a.name nodeName,
			'carSeries' nodeType,
			false expanded,
			true isLeaf
		FROM
			com_car_series a
		WHERE
			a.car_brand_id = #nodeId#</select>
</sqlMap>