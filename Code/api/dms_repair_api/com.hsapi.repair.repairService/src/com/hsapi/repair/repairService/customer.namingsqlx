<sqlMap>
    <parameterMap class="commonj.sdo.DataObject" id="parameterMap">
        <parameter javaType="date" jdbcType="DATE" property="dateType"/>
    </parameterMap>
    <resultMap class="commonj.sdo.DataObject" id="resultMap">
        <result column="TYPEID" javaType="string" property="typeId"/>
    </resultMap>
    <!--
    	LEFT  JOIN rpb_contactor d ON b.id=d.guest_id
    	活跃期（ 近三个月内有消费）、稳定期（ 近三个月无消费、近6个月有消费）、
                    睡眠期（ 近6个月无消费、近1个月有消费）、流失期（ 1年以上无消费）、未分类（无消费历史）
    -->
    <select id="queryCustomerList" parameterClass="java.util.HashMap" resultClass="commonj.sdo.DataObject">
		SELECT
			a.id id,
			a.is_disabled isDisabled,
			b.orgid orgid,
			a.car_no carNo,
			b.id as guestId,
			a.vin vin,
			b.full_name guestFullName,
			a.car_model_id carModelId,
			a.car_model carModel,
			a.car_model_full_name as carModelFullName,
			a.car_brand_id carBrandId,
			a.engine_no engineNo,
			a.annual_inspection_date annualInspectionDate,
			a.annual_verification_due_date annualVerificationDueDate,
			a.insure_comp_code insureCompCode,
			a.insure_comp_name insureCompName,
			a.insure_due_date insureDueDate,
			b.addr addr,
			c.last_come_date lastComeDate,
			c.last_leave_date lastLeaveDate,
			a.recorder recorder,
			a.record_date recordDate,
			a.produce_date produceDate,
			a.color color,
			c.chain_come_times chainComeTimes,
			b.mobile mobile,
			c.mt_advisor_name mtAdvisorName,
			b.tgrade tgrade,
			ifnull( DATEDIFF( now( ), c.last_leave_date ), 0 ) leaveDay
		FROM
			wb_common.com_guest b
		left JOIN rpb_car a ON (a.guest_id = b.id)
		LEFT  JOIN rpb_car_extend c ON a.id=c.car_id
		WHERE
			b.guest_type = '01020103'
		<dynamic>
            <isNotEmpty prepend="and" property="tenantId">  
                (b.tenant_id = #tenantId#)
            </isNotEmpty>  
            <isNotEmpty prepend="and" property="lastOrgid">  
                (c.last_orgid = #lastOrgid#)
            </isNotEmpty> 
            <isNotEmpty prepend="and" property="isDisabled">  
                (a.is_disabled = #isDisabled# )
            </isNotEmpty>     
             <isNotEmpty prepend="and" property="recordStart">  
                (a.record_date &gt;= #recordStart# )
            </isNotEmpty>
            <isNotEmpty prepend="and" property="recordEnd">  
                (a.record_date &lt;= #recordEnd# )
            </isNotEmpty>
            <isNotEmpty prepend="and" property="carModelId">  
                (a.car_model_id = #carModelId#)
            </isNotEmpty>
            <isNotEmpty prepend="and" property="guestId">  
                (a.guest_id = #guestId#)
            </isNotEmpty>
            <isNotEmpty prepend="and" property="mtAdvisorId">  
                (C.mt_advisor_id = #mtAdvisorId#)
            </isNotEmpty>
            <isNotEmpty prepend="and" property="carBrandId">  
                (a.car_brand_id = #carBrandId#)
            </isNotEmpty>
                <isNotEmpty prepend="and" property="carNo">  
                (a.car_no like '%$carNo$%')
            </isNotEmpty>
            <isNotEmpty prepend="and" property="mobile">  
                (b.mobile like '%$mobile$%')
            </isNotEmpty>
              <isNotEmpty prepend="and" property="vin">  
                (a.vin like '%$vin$%')
            </isNotEmpty>
            <isNotEmpty prepend="and" property="guestFullName">  
                (b.full_name like '%$guestFullName$%')
            </isNotEmpty>
            
            <isNotEmpty prepend="and" property="guestTypeId">  
                (b.tgrade in #guestTypeId#)
            </isNotEmpty>
            
            <isNotEmpty prepend="and" property="chainConsumeAmtStar">  
                (c.chain_consume_amt &gt;= #chainConsumeAmtStar# )
            </isNotEmpty>
            <isNotEmpty prepend="and" property="chainConsumeAmtEnd">  
                (c.chain_consume_amt &lt;= #chainConsumeAmtEnd# )
            </isNotEmpty>
            
            <isNotEmpty prepend="and" property="chainComeTimesStar">  
                (c.chain_come_times &gt;= #chainComeTimesStar# )
            </isNotEmpty>
            <isNotEmpty prepend="and" property="chainComeTimesEnd">  
                (c.chain_come_times &lt;= #chainComeTimesEnd# )
            </isNotEmpty>
            
            <isNotEmpty prepend="and" property="annualVerificationDueDateStar">  
                (a.annual_verification_due_date &gt;= #annualVerificationDueDateStar# )
            </isNotEmpty>
            <isNotEmpty prepend="and" property="annualVerificationDueDateEnd">  
                (a.annual_verification_due_date &lt;= #annualVerificationDueDateEnd# )
            </isNotEmpty>
             
            <isNotEmpty prepend="and" property="annualInspectionDate">  
                (a.annual_inspection_date &lt; #annualInspectionDate#)
            </isNotEmpty>
            
            <isNotEmpty prepend="and" property="annualInspectionDateStar">  
                (a.annual_inspection_date &gt;= #annualInspectionDateStar# )
            </isNotEmpty>
            <isNotEmpty prepend="and" property="annualInspectionDateEnd">  
                (a.annual_inspection_date &lt;= #annualInspectionDateEnd# )
            </isNotEmpty>
            
            <isNotEmpty prepend="and" property="insureDueDateStar">  
                (a.insure_due_date &gt;= #insureDueDateStar# )
            </isNotEmpty>
            
             <!--本日所有保险开单客户-->
            <isNotEmpty prepend="and" property="insurance">  
                exists(select 1 from wb_repair.rps_insurance_main d
                     where b.id = d.guest_id and d.record_date &gt;= date_format(DATE_SUB(curdate(),INTERVAL 0 DAY),'%Y-%m-%d %H:%i:%s') )
            </isNotEmpty>
            
            
             <isNotEmpty prepend="and" property="insureDueDateEnd">  
                (a.insure_due_date &lt;= #insureDueDateEnd# )
            </isNotEmpty>
            
             <isNotEmpty prepend="and" property="type">  
             	<!-- 
             		0全部、1活跃期（ 近三个月内有消费）、2稳定期（ 近三个月无消费、近6个月有消费）、
                    3睡眠期（ 近6个月无消费、近1个月有消费）、4流失期（ 1年以上无消费）、5未分类（无消费历史）
             	-->
                <isEqual prepend="and" property="type" compareValue="1">
					and c.last_leave_date  &gt;= date_format(DATE_ADD(curdate(),interval -day(curdate())+1 day),'%Y-%m-%d %H:%i:%s')  
                	and c.last_leave_date  &lt; date_format(date_add(curdate()-day(curdate())+1,interval 1 month),'%Y-%m-%d %H:%i:%s')
				</isEqual>
            </isNotEmpty>
                        
            <!--本日来厂-->
            <isEqual  property="todayEnter" compareValue="1">
					 AND c.last_come_date &gt;= date_format(DATE_SUB(curdate(),INTERVAL 0 DAY),'%Y-%m-%d %H:%i:%s')  
			</isEqual>
			   <!--昨日来厂-->
            <isEqual  property="yesterdayEnter" compareValue="1">  
                    AND c.last_come_date &gt;= date_format(DATE_SUB(curdate(),INTERVAL 1 DAY),'%Y-%m-%d %H:%i:%s')  
                    AND c.last_come_date &lt; date_format(DATE_SUB(curdate(),INTERVAL 0 DAY),'%Y-%m-%d %H:%i:%s')
            </isEqual>
             <!--本日新客户-->
            <isEqual  property="todayNew" compareValue="1">
					 AND c.last_come_date &gt;= date_format(DATE_SUB(curdate(),INTERVAL 0 DAY),'%Y-%m-%d %H:%i:%s')
					 AND ifnull(c.chain_come_times,0) = 1
			</isEqual>
			  <!--本月新客户 --> 
            <isEqual property="thisMonthNew" compareValue="1"> 
                and c.last_come_date  &gt;= date_format(DATE_ADD(curdate(),interval -day(curdate())+1 day),'%Y-%m-%d %H:%i:%s')  
                and c.last_come_date  &lt; date_format(date_add(curdate()-day(curdate())+1,interval 1 month),'%Y-%m-%d %H:%i:%s')
                and ifnull(c.chain_come_times,0) = 1
            </isEqual>
            <!--本月所有来厂客户 --> 
            <isEqual  property="thisMonthEnter" compareValue="1"> 
                and c.last_come_date  &gt;= date_format(DATE_ADD(curdate(),interval -day(curdate())+1 day),'%Y-%m-%d %H:%i:%s')  
                and c.last_come_date  &lt; date_format(date_add(curdate()-day(curdate())+1,interval 1 month),'%Y-%m-%d %H:%i:%s')
            </isEqual>
            <!--最后来厂日期 -->
            <isNotEmpty prepend="and" property="lastEnterStart">  
                (c.last_come_date &gt;= #lastEnterStart#)
            </isNotEmpty>
            <isNotEmpty prepend="and" property="lastEnterEnd">  
                (c.last_come_date &lt; #lastEnterEnd#)
            </isNotEmpty>
             <!--第一次来厂日期 -->
            <isNotEmpty prepend="and" property="firstEnterStart">  
                (c.first_come_date &gt;= #firstEnterStart#)
            </isNotEmpty>
            <isNotEmpty prepend="and" property="firstEnterEnd">  
                (c.first_come_date &lt; #firstEnterEnd#)
            </isNotEmpty>
             <!--最后离厂日期 -->
            <isNotEmpty prepend="and" property="lastOutStart">  
                (c.last_leave_date &gt;= #lastOutStart#)
            </isNotEmpty>
            <isNotEmpty prepend="and" property="lastOutEnd">  
                (c.last_leave_date &lt; #lastOutEnd#)
            </isNotEmpty>
             <!--最后来厂日期 -->
            
            
        </dynamic>
        order by b.modify_date desc
	</select>
	
	
	 <select id="wechatQueryCarByMobile" parameterClass="java.util.HashMap" resultClass="commonj.sdo.DataObject">
		SELECT
			a.id id,
			b.orgid orgid,
			a.car_no carNo,
			b.id as guestId,
			a.vin vin,
			b.full_name fullName,
			a.car_model_id carModelId,
			a.car_model_id_ly carModelIdLy,
			a.car_model carModel,
			a.car_model_full_name as carModelFullName,
			a.car_brand_id carBrandId,
			a.car_series_id carSeriesId
		FROM
			wb_common.com_guest b
		LEFT JOIN rpb_car a ON (a.guest_id = b.id)
		WHERE
			b.guest_type = '01020103'
			and b.mobile = #mobile#
        order by b.modify_date desc
	</select>
	
	
	
