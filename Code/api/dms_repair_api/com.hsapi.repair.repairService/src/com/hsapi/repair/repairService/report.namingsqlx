			sum(b.out_qty) as sellQty,
			sum(b.sell_amt) as sellAmt,
			sum(b.out_return_qty) as sellRtnQty,
			sum(b.out_return_qty*b.sell_unit_price) as sellRtnAmt,
			(sum(b.out_qty)-sum(b.out_return_qty)) as trueQty,
			(sum(b.sell_amt)- sum(b.out_return_qty*b.sell_unit_price))as trueAmt,
			sum(b.true_cost) as trueCost,
			(sum(b.sell_amt)-sum(b.true_cost)) as trueProfitAmt,
