		 <isNotEmpty prepend="and" property="startDate"> 
		      A.outDate &gt;= #startDate#
		 </isNotEmpty>
		 <isNotEmpty prepend="and" property="endDate"> 
		      A.outDate &lt;= #endDate#
		 </isNotEmpty> 
		 <isNotEmpty prepend="and" property="orgid"> 
		      A.orgid = #orgid#
		 </isNotEmpty>   	
    </select>
    
