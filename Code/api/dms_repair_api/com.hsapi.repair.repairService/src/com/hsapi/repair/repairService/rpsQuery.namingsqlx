	<select id="searchContentManagement" parameterClass="java.util.HashMap" resultClass="commonj.sdo.DataObject">
    	SELECT 
			a.id as id,
			a.parent_id As parentId,
			a.name AS name,
			a.introduce AS introduce,
			a.name_sort AS nameSort,
			a.is_disabled AS isDisabled,
