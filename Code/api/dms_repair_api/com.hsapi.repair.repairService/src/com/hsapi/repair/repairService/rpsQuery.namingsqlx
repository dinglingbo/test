			a.out_date as outDate,
			a.is_out_bill as isOutBill,
			a.mt_advisor_id as mtAdvisorId,
			a.guest_desc guestDesc,
			a.fault_phen faultPhen,
			a.solve_method solveMethod,
	    	d.wechat_open_id as openId,
			(select GROUP_CONCAT(name) from wb_repair.rps_service_type e 
	       inner join rpb_business_type f on e.service_type_id = f.id
	       where e.service_id = a.id) as serviceTypeName,
		      ifnull((SELECT sum(g.subtotal) from rps_package g where a.id = g.service_id),0) as pkgSubtotalNo,
		      ifnull((SELECT sum(h.subtotal) from rps_item h where a.id = h.service_id),0) as itemSubtotalNo,
		      ifnull((SELECT sum(i.subtotal) from rps_part i where a.id = i.service_id),0) as partSubtotalNo,
		      ifnull((SELECT sum(g.subtotal) from rps_package g where a.id = g.service_id),0)+
		      ifnull((SELECT sum(h.subtotal) from rps_item h where a.id = h.service_id),0)+
		      ifnull((SELECT sum(i.subtotal) from rps_part i where a.id = i.service_id),0) as totalNo,
			w.package_pref_amt AS packagePrefAmt,
			w.item_pref_amt AS itemPrefAmt,
			w.part_pref_amt AS partPrefAmt,
			w.other_amt AS otherAmt,
			w.package_subtotal+w.item_subtotal+w.part_subtotal+w.other_amt AS incomeTotal,
			w.allowance_amt as allowanceAmt,
			w.card_times_amt AS cardTimesAmt,
			w.total_pref_amt AS totalPrefAmt,
			w.bala_amt AS balaAmt,		
			w.other_cost_amt AS otherCostAmt,
			w.part_true_cost AS partTrueCost,
			w.part_no_tax_cost AS partNoTaxCost,
			w.part_tax_cost AS partTaxCost,
			w.tech_deduct_value AS techDeductValue,
			w.sales_deduct_value AS salesDeductValue,
			w.advisor_deduct_value AS advisorDeductValue,			
			w.advisor_deduct_value+w.sales_deduct_value+w.tech_deduct_value+w.part_true_cost+w.other_cost_amt AS expenditureTotal,			
			w.total_deduct_amt AS totalDeductAmt,
			w.gross_profit AS grossProfit,
			w.gross_profit_rate AS grossProfitRate,
			w.gross_profit_remark AS grossProfitRemark,
			w.netin_amt AS netinAmt,
			w.total_pref_rate AS totalPrefRate,
			w.record_date as recordDate
		from rps_maintain a
		inner JOIN rpb_car b ON ( a.car_id = b.id )
		INNER JOIN wb_common.com_guest c on a.guest_id = c.id
		inner join rpb_contactor d on (a.contactor_id = d.id and c.id = d.guest_id)	 
		left join rps_settlement w on (a.id =  w.service_id)
