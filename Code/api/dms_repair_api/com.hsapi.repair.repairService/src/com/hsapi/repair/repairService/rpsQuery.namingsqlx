	FROM wb_slog.sys_list_information a
		INNER JOIN sys_content_management b
			ON a.id = b.main_id
			
		WHERE 1 = 1
		<dynamic>
			<isNotEmpty prepend="and" property="parentId">
                (a.parent_id = #parentId#)
            </isNotEmpty>
        </dynamic>  
        <dynamic>
			<isNotEmpty prepend="and" property="id">
                (a.id = #id#)
            </isNotEmpty>
        </dynamic> 
        <dynamic>
			<isNotEmpty prepend="and" property="rid">
                (b.id = #rid#)
            </isNotEmpty>
        </dynamic> 
        ORDER BY parent_id asc
	</select>
	
	
	<select id="queryRpsItemList" parameterClass="java.util.HashMap" resultClass="commonj.sdo.DataObject">
    	SELECT a.service_code as serviceCode, a.car_no as carNo, a.car_vin as carVin,
    	       b.id as id, 
    	       a.id as serviceId,
    	       b.stop_reason as stopReason,
    	       b.back_reason as backReason,
    	       b.is_back as isBack,
    	       b.stop_time as stopTime,
    	       b.work_time as workTime,
    	       b.back_times as backTimes,
    	       b.worker_ids as workerIds,
    	       b.checker_ids as checkerIds,
    	       b.checkers as checkers,
    	       b.begin_date as beginDate,
       		   b.item_name as itemName, b.workers as workers, 
       		   b.status as status, b.finish_auditor as finisher, b.finish_date as finishDate
		from rps_maintain a
		inner join rps_item b
		on a.id = b.service_id
		where 1 = 1
		      and a.status = 1
		      and a.orgid = #orgid#
		      <dynamic>
					<isNotEmpty prepend="and" property="status">
		                (b.`status` = #status#)
		            </isNotEmpty>
		            <isNotEmpty prepend="and" property="isFinish">
		                (b.`status` = 3)
		            </isNotEmpty>
		             <isNotEmpty prepend="and" property="notFinish">
		                (b.`status` != 3)
		            </isNotEmpty>
		            <isNotEmpty prepend="and" property="carNo">  
						a.car_no like '%$carNo$%'
					</isNotEmpty>
					
					<isNotEmpty prepend="and" property="serviceCode">  
						a.service_code like '%$serviceCode$%'
					</isNotEmpty>
		
		
	          </dynamic>
		      
		       and exists(select 1 from rps_item_workers c 
		      			where b.id = c.main_id 
		      			    <dynamic>
								<isNotEmpty prepend="and" property="workerId">
					                (c.worker_id = #workerId#)
					            </isNotEmpty>
					        </dynamic>  
					        )
		order by b.modify_date desc
	
	</select>
	
	
