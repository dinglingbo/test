	<!--预收 计次卡购买 -->
	<select id="queryCardTimesAmt" parameterClass="java.util.HashMap" resultClass="commonj.sdo.DataObject">
    	SELECT sum(ifnull(sell_amt,0)) as amt from rps_card_times
		where status = 2 
