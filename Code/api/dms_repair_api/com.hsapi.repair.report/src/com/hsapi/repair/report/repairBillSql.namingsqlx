		      SELECT
		         a.orgid  orgid, 
       		     sum(b.can_use_times) as canUseTimesTotal,
       		     sum(b.sell_price * b.can_use_times) as amtTotal,
       		     c.short_name shortName
                 from wb_common.com_company c
                 inner join  rps_card_times a on   a.orgid = c.orgid
