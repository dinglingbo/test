       <!--采购   删除pj_pchs_order_main + detail-->
    <delete id="deletePjPchsOrderMain" parameterClass="HashMap">
     DELETE wb_part.pj_pchs_order_main , wb_part.pj_pchs_order_detail  FROM wb_part.pj_pchs_order_main 
			LEFT JOIN wb_part.pj_pchs_order_detail on wb_part.pj_pchs_order_main.id = wb_part.pj_pchs_order_detail.main_id
 			where wb_part.pj_pchs_order_main.orgid = #orgid#
