     <!--供应商   删除com_guest-->
    <delete id="deleteComGuest" parameterClass="HashMap">
     DELETE wb_common.com_guest , wb_repair.rpb_guest FROM wb_common.com_guest	
     		LEFT JOIN wb_common.com_guest on wb_common.com_guest.id =wb_repair.rpb_guest.id		
 			where wb_common.com_guest.orgid = #orgid# and wb_common.com_guest.is_supplier = 1
    </delete> 
    
         <!--客户   删除com_guest-->
    <delete id="deleteGuest" parameterClass="HashMap">
     DELETE wb_common.com_guest , wb_repair.rpb_guest FROM wb_common.com_guest	
     		LEFT JOIN wb_common.com_guest on  wb_repair.rpb_guest.id = wb_common.com_guest.id	
 			where wb_common.com_guest.orgid = #orgid# and wb_common.com_guest.is_client = 1 and wb_common.com_guest.guest_type = '01020103'
    </delete>   
